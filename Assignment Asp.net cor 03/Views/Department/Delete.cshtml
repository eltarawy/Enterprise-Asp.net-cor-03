@model Department
@{
    ViewData["Title"] = "Delete";
}
<form asp-action="Delete">
    <div asp-validation-summary="All"></div>
    <h1>Are yuo shore to Delete this department?</h1>
    <div class="form-group">
        <label class="col-form-label" asp-for="Code"></label>
        <input class="form-control" asp-for="Code" readonly />
        <span asp-validation-for="Code" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input class="form-control" asp-for="Name" readonly />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="DateOfCreation"></label>
        <input class="form-control" asp-for="DateOfCreation" readonly />
        <span asp-validation-for="DateOfCreation" class="text-danger"></span>
    </div>

    <input type="submit" value="Delete" class="btn btn-danger">||
    <a asp-action="Index" class=" btn btn-primary">Cancel</a>
</form>
